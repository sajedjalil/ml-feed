{
  "name": "ml-feed",
  "displayName": "ML feed",
  "description": "Notebook for running REST queries.",
  "version": "1.0.0",
  "publisher": "sajedjalil",
  "repository": {
    "url": "https://github.com/sajedjalil/ml-feed"
  },
  "enabledApiProposals": [
    "notebookEditor"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Notebooks"
  ],
  "icon": "ml-feed.png",
  "activationEvents": [
    "onNotebook:ml-feed",
    "onNotebook:interactive",
    "onNotebook:jupyter-notebook",
    "onNotebook:interactive",
    "onLanguage:ml-feed",
    "onLanguage:python",
    "onLanguage:jupyter",
    "onCommand:ml-feed.newInteractive",
    "onCommand:ml-feed.newNotebook"
  ],
  "main": "./dist/extension-node.js",
  "contributes": {
    "commands": [
      {
        "command": "ml-feed.newInteractive",
        "title": "Create an Interactive Window",
        "shortTitle": "Interactive REST Window",
        "category": "ML feed"
      },
      {
        "command": "ml-feed.newNotebook",
        "title": "Create a new blank REST Notebook",
        "shortTitle": "REST Notebook",
        "category": "ML feed"
      }
    ],
    "menus": {
      "file/newFile": [
        {
          "command": "ml-feed.newInteractive",
          "group": "notebook"
        },
        {
          "command": "ml-feed.newNotebook",
          "group": "notebook"
        }
      ]
    },
    "notebookRenderer": [
      {
        "id": "ml-feed",
        "displayName": "ML feed Renderer",
        "entrypoint": "./dist/renderer.js",
        "requiresMessaging": "optional",
        "mimeTypes": [
          "x-application/ml-feed"
        ]
      }
    ],
    "notebooks": [
      {
        "id": "ml-feed",
        "type": "ml-feed",
        "displayName": "ML feed",
        "selector": [
          {
            "filenamePattern": "*.notebook"
          }
        ]
      }
    ],
    "languages": [
      {
        "id": "ml-feed",
        "aliases": [
          "ML feed"
        ],
        "filenamePatterns": [
          "*.ml-feed.txt"
        ]
      }
    ],
    "grammars": [
      {
        "language": "ml-feed",
        "scopeName": "source.ml-feed",
        "path": "./syntaxes/ml-feed.tmGrammar.json",
        "embeddedLanguages": {
          "meta.embedded.block.python": "python",
          "meta.embedded.block.javascript": "javascript",
          "meta.embedded.block.json": "json",
          "meta.embedded.block.html": "html",
          "meta.embedded.block.xml": "xml",
          "meta.embedded.block.css": "css"
        }
      }
    ],
    "snippets": [
      {
        "language": "ml-feed",
        "path": "./snippets/snippets.code-snippets"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch  --info-verbosity verbose",
    "lint": "eslint --config .eslintrc.json ./src/**/*.ts",
    "ts-compile": "tsc -b",
    "integration-test": "npm run ts-compile && node ./out/extension/test-integration/runTest.js",
    "unit-test": "npm run ts-compile && npx mocha ./out/extension/test-unit",
    "compile-lint-test": "npm run compile && npm run lint && npm run unit-test",
    "precommit": "npm run lint",
    "deploy": "npx vsce publish --web --noVerify",
    "update-api": "npx vscode-dts dev"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.1",
    "@types/node": "^12.20.10",
    "@types/shelljs": "^0.8.11",
    "@types/user-home": "^2.0.0",
    "@types/uuid": "^8.3.0",
    "@types/vscode": "^1.59.0",
    "@types/vscode-notebook-renderer": "^1.57.8",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "axios": "^0.21.1",
    "css-loader": "^5.1.3",
    "eslint": "^7.22.0",
    "glob": "^7.1.6",
    "json-stringify-safe": "^5.0.1",
    "mocha": "^8.3.2",
    "os": "^0.1.1",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "shelljs": "^0.8.5",
    "style-loader": "^2.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "url": "^0.11.0",
    "util": "^0.12.3",
    "vscode-codicons": "0.0.14",
    "vscode-dts": "^0.3.2",
    "vscode-test": "^1.5.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "-": "0.0.1",
    "@types/react": "^17.0.3",
    "lodash": "^4.17.21",
    "preact": "^10.5.13",
    "save-dev": "0.0.1-security",
    "user-home": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
